    // AGG__MAX(modelToAgg,fieldToAgg,countblanks)
    function(modelname, fieldname, blanks) { 
        return skuid.aggregations.aggregate("max",skuid.$M(modelname), fieldname, {
            countBlanks: blanks
        });
    },
    {
        namespace: "MATH",
